set terminal png
set output ofilename

set grid x y
set key left top title " "
set title "Parallelism"
set xlabel "unique keys"
set xrange [0 : 10000000]
set ylabel "Mio. Operations/sec"
set xtics 1000000
set tics out

plot filename1 using 2:3 with line title 'no local ht' , filename2 using 2:3 with line title 'hyperlike' , filename3 using 2:3 with line title 'global partitions(lock)'
